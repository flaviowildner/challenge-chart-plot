{
	"name": "desafio-intelie",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"brace": "^0.11.1",
		"chart.js": "^2.7.3",
		"concurrently": "^4.1.0",
		"node-sass": "^4.11.0",
		"react": "^16.8.2",
		"react-chartjs-2": "^2.7.4",
		"react-chartkick": "^0.3.0",
		"react-dom": "^16.8.2",
		"react-monaco-editor": "^0.24.0",
		"react-scripts": "2.1.5"
	},
	"scripts": {
		"watch-scss": "node-sass -rw src -o  src",
		"build-css": "node-sass -r src -o src",
		"start": "concurrently \"npm run build-css\" \"npm run watch-scss\" \"npm run start-react\"",
		"start-react": "react-scripts start",
		"build": "npm run build-css && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
